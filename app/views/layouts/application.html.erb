<script>
    $(document).ready(function() {
        $('#fullpage').fullpage({
            sectionsColor: ['#1bbc9b', '#CC0000', '#808080', '#207AA0','#995C1F', '#FFFFFF','#222222'],
            anchors: ['firstPage', 'secondPage', '3rdPage','4thPage','5thPage','6thPage','7thPage'],
            menu: '#menu',
            autoScrolling: true,
            fitToSection: false,
            afterRender: function(){
                //playing the video
                $('#myVideo').get(0).play();
            },
            afterSlideLoad: function(anchorLink, index, slideAnchor, slideIndex){
                var animation = "animated shake";
                var animationStop = "webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend";
                if(index==2 && slideIndex==2){
                    $('.arrow_image').addClass(animation);
                }
                else if(index==2 && slideIndex ==1){
                    $('.finger_one, .finger_two').addClass("animated pulse");
                }
                else if(index==2 && slideIndex ==3){
                    $('.voice_image').addClass('animation tada');
                }
            }

        });
    });
</script>

